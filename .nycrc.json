{
  "all": true,
  "check-coverage": true,
  "branches": 80,
  "functions": 80,
  "lines": 80,
  "statements": 80,
  "include": [
    "*.js",
    "scripts/*.js"
  ],
  "exclude": [
    "tests/**",
    "node_modules/**",
    "coverage/**",
    "ecosystem.config.cjs",
    "utils/**"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./coverage/.nyc_output",
  "instrument": true,
  "sourceMap": false,
  "cache": false
}
